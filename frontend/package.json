{
  "name": "frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=11.0.0"
  },
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@expo-google-fonts/lora": "^0.4.2",
    "@expo-google-fonts/pt-serif": "^0.4.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.11.0",
    "axios": "^1.13.4",
    "expo": "~54.0.32",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "rgba": "^0.0.0",
    "ts-node": "^10.9.2",
    "victory-native": "36.9.2",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.16",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@sentry/.*|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "private": true
}
